%h1 New group
- form_for @group, :html => {:multipart => true} do |f|
  = f.error_messages
  = f.label :name
  %br
  = f.text_field :name
  %br
  %br
  = f.label :purpose
  %br
  = f.text_field :purpose
  %br
  %br
  = f.label :dues
  %br
  = f.text_field :dues
  %br
  %br
  = f.label :photo
  %br
  = f.file_field :photo
  %p.prepend-top= f.submit 'Create'
= link_to 'Back', groups_path
